{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ahmadmarafa/laravel-sendgrid-notification-channel"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "barryvdh/laravel-ide-helper": "^2.6",
        "brozot/laravel-fcm": "^1.2",
        "cuonggt/laravel-sendgrid-notification-channel": "dev-master",
        "dimsav/laravel-translatable": "^9.4",
        "doctrine/dbal": "^2.9",
        "felixkiss/uniquewith-validator": "^3.4",
        "fideloper/proxy": "^4.0",
        "funkjedi/composer-include-files": "^1.0",
        "jenssegers/mongodb": "^3.5",
        "kalnoy/nestedset": "^4.3",
        "laravel/framework": "5.8.*",
        "laravel/passport": "^7.2",
        "laravel/tinker": "^1.0",
        "league/flysystem-cached-adapter": "^1.0",
        "maatwebsite/excel": "^3.1",
        "nexmo/client": "^1.8",
        "tightenco/ziggy": "^0.7.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "include_files": [
            "app/Http/Helpers.php"
        ]
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
